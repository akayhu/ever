    "vendor": "webpack --config src/configs/ddl.config.js --progress --colors",
    "export": "webpack --config src/configs/ddl.client.js --progress --colors",
    "postinstall": "npm run babel",
    "test": "export NODE_ENV=dev && jest --env=node",
    "test:watch": "npm test -- --watch"
